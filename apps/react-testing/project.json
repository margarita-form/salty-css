{
  "name": "react-testing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-testing/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/react-testing"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "staging": {
          "mode": "production",
          "fileReplacements": [
            {
              "replace": "apps/react-testing/environments/environment.ts",
              "with": "apps/react-testing/environments/environment-staging.ts"
            }
          ]
        },
        "production": {
          "mode": "production",
          "fileReplacements": [
            {
              "replace": "apps/react-testing/environments/environment.ts",
              "with": "apps/react-testing/environments/environment-production.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-testing:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-testing:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "react-testing:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-testing:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "react-testing:build:development"
        },
        "production": {
          "buildTarget": "react-testing:build:production"
        }
      },
      "dependsOn": ["build"]
    }
  }
}
