{
  "name": "@salty-css/eslint-config-core",
  "version": "0.0.1-alpha.238",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "ESLint plugin for Salty CSS",
  "homepage": "https://salty-css.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/margarita-form/salty-css.git"
  },
  "bugs": {
    "url": "https://github.com/margarita-form/salty-css/issues"
  },
  "files": [
    "**/*",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/eslint-config-core/src",
    "name": "eslint-config-core"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./flat": {
      "import": "./flat.js",
      "require": "./flat.cjs"
    }
  },
  "dependencies": {
    "@salty-css/core": "^0.0.1-alpha.238",
    "@salty-css/eslint-plugin-core": "^0.0.1-alpha.238",
    "eslint": ">=9.x || >=8.x || >=7.x"
  }
}
