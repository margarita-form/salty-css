{
  "name": "@salty-css/react",
  "version": "0.0.1-alpha.259",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "React library for Salty CSS",
  "homepage": "https://salty-css.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/margarita-form/salty-css.git"
  },
  "bugs": {
    "url": "https://github.com/margarita-form/salty-css/issues"
  },
  "files": [
    "**/*",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/react/src",
    "name": "react"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./styled": {
      "import": "./styled.js",
      "require": "./styled.cjs"
    },
    "./styled-client": {
      "import": "./styled-client.js",
      "require": "./styled-client.cjs"
    },
    "./class-name": {
      "import": "./class-name.js",
      "require": "./class-name.cjs"
    },
    "./class-name-client": {
      "import": "./class-name-client.js",
      "require": "./class-name-client.cjs"
    },
    "./keyframes": {
      "import": "./keyframes.js",
      "require": "./keyframes.cjs"
    },
    "./media": {
      "import": "./media.js",
      "require": "./media.cjs"
    },
    "./config": {
      "import": "./config.js",
      "require": "./config.cjs"
    },
    "./helpers": {
      "import": "./helpers.js",
      "require": "./helpers.cjs"
    }
  },
  "dependencies": {
    "@salty-css/core": "^0.0.1-alpha.259",
    "clsx": ">=2.x",
    "react": ">=19.x || >=18.3.x"
  }
}
