{
  "name": "@salty-css/core",
  "version": "0.0.1-alpha.290",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Core library for Salty CSS. Will be used by other Salty CSS libraries and plugins.",
  "homepage": "https://salty-css.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/margarita-form/salty-css.git"
  },
  "bugs": {
    "url": "https://github.com/margarita-form/salty-css/issues"
  },
  "files": [
    "**/*",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/core/src",
    "name": "core"
  },
  "peerDependencies": {
    "color": ">=5.x",
    "commander": ">=12.x",
    "csstype": "^3.1.x",
    "ejs": ">=3.x",
    "esbuild": ">=0.25.x",
    "ora": ">=8.x",
    "react": ">=18.x || >=19.x",
    "typescript": ">=5.x",
    "winston": ">=3.x"
  },
  "exports": {
    "./bin": {
      "import": "./bin/index.js",
      "require": "./bin/index.cjs"
    },
    "./bin/main": {
      "import": "./bin/main.js",
      "require": "./bin/main.cjs"
    },
    "./compiler": {
      "import": "./compiler/index.js",
      "require": "./compiler/index.cjs"
    },
    "./factories": {
      "import": "./factories/index.js",
      "require": "./factories/index.cjs"
    },
    "./css": {
      "import": "./css/index.js",
      "require": "./css/index.cjs"
    },
    "./css/keyframes": {
      "import": "./css/keyframes.js",
      "require": "./css/keyframes.cjs"
    },
    "./css/media": {
      "import": "./css/media.js",
      "require": "./css/media.cjs"
    },
    "./css/token": {
      "import": "./css/token.js",
      "require": "./css/token.cjs"
    },
    "./css/merge": {
      "import": "./css/merge.js",
      "require": "./css/merge.cjs"
    },
    "./helpers": {
      "import": "./helpers/index.js",
      "require": "./helpers/index.cjs"
    },
    "./generators": {
      "import": "./generators/index.js",
      "require": "./generators/index.cjs"
    },
    "./parsers": {
      "import": "./parsers/index.js",
      "require": "./parsers/index.cjs"
    },
    "./config": {
      "import": "./config/index.js",
      "require": "./config/index.cjs"
    },
    "./types": {
      "import": "./types/index.js",
      "require": "./types/index.cjs"
    },
    "./util": {
      "import": "./util/index.js",
      "require": "./util/index.cjs"
    },
    "./server": {
      "import": "./server/index.js",
      "require": "./server/index.cjs"
    },
    "./cache/resolve-dynamic-config-cache": {
      "import": "./cache/resolve-dynamic-config-cache.js",
      "require": "./cache/resolve-dynamic-config-cache.cjs"
    }
  },
  "bin": {
    "salty": "./bin/index.js",
    "salty-css": "./bin/index.js",
    "salty-css-cjs": "./bin/index.cjs",
    "@salty-css/core": "./bin/index.js"
  }
}
